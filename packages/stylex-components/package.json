{
  "name": "@tw-to-stylex/components",
  "version": "0.1.0",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./styles": {
      "import": "./dist/styles.css"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsup src/index.tsx --format esm --dts --external react --external react-dom --external @stylexjs/stylex",
    "dev": "tsup src/index.tsx --format esm --dts --external react --external react-dom --external @stylexjs/stylex --watch"
  },
  "peerDependencies": {
    "@stylexjs/stylex": ">=0.17.0",
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "devDependencies": {
    "@stylexjs/babel-plugin": "^0.17.5",
    "@stylexjs/stylex": "^0.17.5",
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.3.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tsup": "^8.0.1",
    "typescript": "^5.4.2"
  }
}
